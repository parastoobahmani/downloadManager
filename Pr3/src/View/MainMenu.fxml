               <font>
                  <Font name="Algerian" size="17.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#ButtonDocumentsOnAction" prefHeight="90.0" prefWidth="163.0" style="-fx-background-radius: 45; -fx-border-radius: 45; -fx-border-color: #b81414; -fx-border-width: 3;" text="Documents" textFill="#b81414">
               <font>
                  <Font name="Algerian" size="21.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#ButtonMediaOnAction" prefHeight="90.0" prefWidth="163.0" style="-fx-border-color: #b81414; -fx-border-radius: 45; -fx-border-width: 3; -fx-background-radius: 45;" text="Media" textFill="#b81414">
               <font>
                  <Font name="Algerian" size="21.0" />
