                              <AnchorPane fx:id="AnchorPaneNewDownload" layoutX="37.0" layoutY="127.0" prefHeight="200.0" prefWidth="503.0" style="-fx-background-color: white;" visible="false">
                                 <children>
                                    <Button layoutX="228.0" layoutY="132.0" mnemonicParsing="false" onAction="#ButtonAddURLOnAction" style="-fx-border-color: Black;" text="Add">
                                       <font>
                                          <Font name="System Bold" size="15.0" />
                                       </font>
                                    </Button>
                                    <TextField fx:id="TxtFieldEnterUrl" layoutX="32.0" layoutY="53.0" prefHeight="31.0" prefWidth="436.0" promptText="Enter The URL" style="-fx-border-color: Black;" />
