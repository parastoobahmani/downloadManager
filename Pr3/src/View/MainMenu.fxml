          </Tab>
            <Tab text="Failed Downloads">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
            <Tab text="Search">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextField layoutX="40.0" layoutY="14.0" prefHeight="31.0" prefWidth="496.0" promptText="Type The Name Of The File You're Looking For" style="-fx-border-color: black; -fx-border-radius: 45; -fx-background-radius: 45;" />
                        <ListView layoutY="63.0" prefHeight="448.0" prefWidth="576.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="63.0" />
